let canvasSize = 400;

function setup() {
  createCanvas(canvasSize, canvasSize);
  background(255);
}

function draw() {
  // Draw sushi roll
  drawRice();
  drawNori();
  drawFish();
}

function drawRice() {
  fill(255, 255, 204); // Rice color
  noStroke();
  ellipse(canvasSize / 2, canvasSize / 2, 150, 150); // Rice base
}

function drawNori() {
  fill(0); // Nori color
  noStroke();
  rect(canvasSize / 2 - 75, canvasSize / 2 - 75, 150, 150); // Nori wrap
}

function drawFish() {
  fill(255, 0, 0); // Fish color (assuming it's salmon)
  noStroke();
  ellipse(canvasSize / 2, canvasSize / 2, 120, 50); // Fish slice
}
